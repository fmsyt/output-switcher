{
  "name": "output-switcher",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "tauri": "tauri",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile build/smui-dark.css -i src/theme/dark"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@smui/select": "7.0.0-beta.18",
    "@smui/slider": "7.0.0-beta.18",
    "@tauri-apps/api": "^1.5.3",
    "svelte-material-icons": "^3.0.5"
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@smui/icon-button": "7.0.0-beta.16",
    "@smui/layout-grid": "7.0.0-beta.16",
    "@smui/paper": "7.0.0-beta.16",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tauri-apps/cli": "^1.5.11",
    "@tsconfig/svelte": "^5.0.4",
    "smui-theme": "7.0.0-beta.16",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.9",
    "svelte-material-ui": "7.0.0-beta.16",
    "tslib": "^2.6.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8"
  }
